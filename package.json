{
  "name": "prisma-type-generator",
  "version": "2.0.3",
  "description": "Prisma type generator for generating types from Prisma schema, with support for JSON type mapping and split by schema and many more.",
  "main": "dist/bin.js",
  "types": "./dist/bin.d.ts",
  "private": false,
  "exports": {
    "require": "./dist/bin.js",
    "default": "./dist/bin.js"
  },
  "files": [
    "dist",
    "generator-config.schema.json"
  ],
  "bin": {
    "prisma-type-generator": "dist/bin.js"
  },
  "scripts": {
    "run": "npm run build && npm link",
    "clear": "rimraf dist",
    "lint": "tsc",
    "build": "npm run clear && npm run build:cjs && npm run build:dts",
    "build:cjs": "esbuild src/bin.ts src/generator.ts src/on-generate.ts src/on-manifest.ts src/config.ts src/util.ts src/generators/*.ts --bundle --outdir=dist --platform=node --format=cjs --outbase=src --minify --external:@prisma/generator-helper --external:@prisma/client",
    "build:dts": "tsc --declaration --emitDeclarationOnly"
  },
  "peerDependencies": {
    "@prisma/client": ">=5.0.0 || >=6.0.0 || >=7.0.0",
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": false
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@prisma/client": "^7.1.0",
    "@prisma/generator-helper": "^7.0.0",
    "@types/node": "^20.11.17",
    "changesets": "^1.0.2",
    "esbuild": "^0.24.2",
    "prisma": "^7.1.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "prisma",
    "typescript",
    "prisma-types",
    "types",
    "interface",
    "monorepo",
    "prisma.d.ts",
    "zod",
    "dto",
    "type-guards",
    "utility-types",
    "schema-generator"
  ],
  "author": {
    "name": "Nischal",
    "url": "https://github.com/broisnischal/prisma-type-generator",
    "email": "nischaldahal01395@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/broisnischal/prisma-type-generator#readme"
}