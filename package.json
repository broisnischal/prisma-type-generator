{
  "name": "prisma-type-generator",
  "version": "1.10.0",
  "description": "Prisma type generator",
  "main": "dist/bin.js",
  "types": "./dist/bin.d.ts",
  "module": "./dist/bin.mjs",
  "private": false,
  "files": [
    "dist"
  ],
  "bin": {
    "prisma-type-generator": "dist/bin.js"
  },
  "scripts": {
    "run": "npm run build && npm link",
    "build": "tsc",
    "build:tsup": "tsup src/bin.ts --format cjs,esm --dts",
    "lint": "tsc"
  },
  "peerDependencies": {
    "@prisma/client": "^6.2.1",
    "@prisma/generator-helper": "^6.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@prisma/generator-helper": "^6.2.1",
    "@types/node": "^20.11.17",
    "changesets": "^1.0.2",
    "prettier": "^3.2.5",
    "prisma": "^6.2.1",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "prisma",
    "typescript",
    "prisma-types",
    "types",
    "interface",
    "database"
  ],
  "author": {
    "name": "Nischal",
    "url": "https://github.com/broisnischal/prisma-type-generator",
    "email": "neeswebservice@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/broisnischal/prisma-type-generator#readme"
}
